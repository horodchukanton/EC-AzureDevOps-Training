<editor>
<help>/commander/pages/@PLUGIN_NAME@/help?s=Administration&amp;ss=Plugins#GetDefaultValues</help>
<formElement>
  <documentation>Previously defined plugin configuration.</documentation>
  <label>Configuration:</label>
  <property>config</property>
  <configuration>1</configuration>
  <propertyReference>/plugins/@PLUGIN_NAME@/project/ec_plugin_cfgs</propertyReference>
  <required>1</required>
  <type>entry</type>
</formElement>
<formElement>
  <documentation>Name or ID of a team project where the work item type is defined.</documentation>
  <label>Project:</label>
  <property>project</property>
  <required>1</required>
  <type>entry</type>
  <value/>
</formElement>
<formElement>
  <documentation>Name of the work item type. Example: 'Feature', 'Bug'</documentation>
  <htmlDocumentation>Name of the work item type.<br/> Example: <b>Feature</b>, <b>Bug</b></htmlDocumentation>
  <label>Work Item Type:</label>
  <property>workItemType</property>
  <propertyReference>/plugins/@PLUGIN_NAME@/project/ecp_azuredevops_workitemtypes</propertyReference>
  <required>1</required>
  <type>entry</type>
  <value/>
</formElement>
<formElement>
  <documentation>Results will be saved into this property/property sheet.</documentation>
  <label>Result Property Sheet:</label>
  <property>resultPropertySheet</property>
  <required>1</required>
  <type>entry</type>
  <value>/myJob/defaultValues</value>
</formElement>
<formElement>
  <documentation>Format to save the request results.</documentation>
  <label>Result Format:</label>
  <option>
    <name>Property Sheet</name>
    <value>propertySheet</value>
  </option>
  <option>
    <name>JSON</name>
    <value>json</value>
  </option>
  <property>resultFormat</property>
  <value>propertySheet</value>
  <required>1</required>
  <type>select</type>
</formElement>

</editor>